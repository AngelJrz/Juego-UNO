<Window x:Class="UNOGui.Ventanas.MenuPrincipal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UNOGui.Ventanas"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MenuPrincipal" Height="500" Width="800">
    <Grid>

        <Image Source="/Imagenes/imagenMenuPrincipal.jpg" Margin="-104,-28,-221,-26"/>
        <Button Content="Mi perfil" Style="{StaticResource Button}" HorizontalAlignment="Left" Margin="27,142,0,0" VerticalAlignment="Top" Width="137" Click="IrAMiPerfil"/>
        <Button Content="Cear Sala" Style="{StaticResource Button}" HorizontalAlignment="Left" Margin="27,90,0,0" VerticalAlignment="Top" Width="137" Click="IrACrearSala"/>
        <Button Content="Unirse a sala" Style="{StaticResource Button}" HorizontalAlignment="Left" Margin="27,38,0,0" VerticalAlignment="Top" Width="137" Click="IrAUnirseASala"/>
        <Button Content="Cerrar sesión" Style="{StaticResource Button}" HorizontalAlignment="Left" Margin="639,38,0,0" VerticalAlignment="Top" Width="121" Click="CerrarSecion"/>

    </Grid>
</Window>
